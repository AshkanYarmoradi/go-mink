---
layout: null
permalink: /assets/js/search-data.json
---
{
{%- assign defined_titles = site.html_pages | where_exp: "item", "item.title != nil" -%}
{%- assign defined_docs = defined_titles | where_exp: "item", "item.nav_exclude != true" -%}
{%- assign defined_search = defined_docs | where_exp: "item", "item.search_exclude != true" -%}
{%- assign docs = defined_search | sort: "title" -%}
{%- assign search_content_heading = "" -%}
{%- for doc in docs -%}
{%- assign doc_heading_count = 0 -%}
{%- assign doc_content_without_html = doc.content | strip_html -%}
{%- assign headings = doc.content | split: '<h' -%}
{%- assign first_heading = true -%}
{%- for heading in headings -%}
{%- assign heading_level = heading | slice: 0, 1 -%}
{%- assign valid_heading = false -%}
{%- if heading_level == "1" or heading_level == "2" or heading_level == "3" or heading_level == "4" or heading_level == "5" or heading_level == "6" -%}
{%- assign valid_heading = true -%}
{%- endif -%}
{%- if valid_heading -%}
{%- if first_heading -%}
{%- assign first_heading = false -%}
{%- continue -%}
{%- endif -%}
{%- assign heading_content = heading | split: '</h' | first -%}
{%- assign heading_text_start = heading_content | split: '>' -%}
{%- assign heading_text_start_size = heading_text_start | size | minus: 1 -%}
{%- assign heading_text_start_last = heading_text_start | last -%}
{%- assign heading_text = heading_text_start_last | strip_html | strip_newlines | normalize_whitespace -%}
{%- assign heading_id_start = heading_content | split: 'id="' | last -%}
{%- assign heading_id = heading_id_start | split: '"' | first -%}
{%- unless heading_text == blank -%}
{%- assign search_content_heading = search_content_heading | append: '"' | append: doc_heading_count | append: '": {
    "doc": "' | append: doc.title | append: '",
    "title": "' | append: heading_text | replace: '\', '\\' | replace: '"', '\"' | append: '",
    "content": "' -%}
{%- assign heading_content_without_html = heading_content | split: '>' | shift | join: '>' | strip_html | strip_newlines | normalize_whitespace | replace: '\', '\\' | replace: '"', '\"' -%}
{%- assign search_content_heading = search_content_heading | append: heading_content_without_html | append: '",
    "url": "' | append: doc.url | append: '#' | append: heading_id | append: '",
    
    "relUrl": "' | append: doc.url | append: '#' | append: heading_id | append: '"
  },' -%}
{%- assign doc_heading_count = doc_heading_count | plus: 1 -%}
{%- endunless -%}
{%- endif -%}
{%- endfor -%}
"{{ doc_heading_count }}": {
    "doc": "{{ doc.title | replace: '\', '\\' | replace: '"', '\"' }}",
    "title": "{{ doc.title | replace: '\', '\\' | replace: '"', '\"' }}",
    "content": "{{ doc_content_without_html | strip_newlines | normalize_whitespace | replace: '\', '\\' | replace: '"', '\"' }}",
    "url": "{{ doc.url }}",
    
    "relUrl": "{{ doc.url }}"
  }{% unless forloop.last %},{% endunless %}
{{ search_content_heading }}
{%- assign search_content_heading = "" -%}
{%- endfor -%}
}
